if ($object->getStatus() != $cfg['status_id']['submitted']) {
    $obj['tabbed_box']->deleteTab('Accept');
}

if ($object->getStatus() != $cfg['status_id']['default']) {
    $obj['tabbed_box']->deleteTab('Submit');
