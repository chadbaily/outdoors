include_once("DatabaseObject.php");

class ItemAttribute extends DatabaseObject {
    // {{{declarations
    var $c_item = null;
    var $c_name = null;
    var $c_value = null;
    // }}}

    /* {{{constructor
     *
     */
    function ItemAttribute() {
        $this->DatabaseObject();
        $this->table = "[_]item_attribute";
    } //}}}

    /* {{{getItem
     *
     */
    function getItem() {
        return $this->c_item;
    } //}}}
    
    /* {{{setItem
     *
     */
    function setItem($value) {
        $this->c_item = $value;
    } //}}}

    /* {{{getName
     */
    function getName() {
        return $this->c_name;
    } //}}}

    /* {{{setName
     */
    function setName($value) {
        $this->c_name = $value;
    } //}}}

    /* {{{getValue
     */
    function getValue() {
        return $this->c_value;
    } //}}}

    /* {{{setValue
     */
    function setValue($value) {
        $this->c_value = $value;
    } //}}}

}
?>
