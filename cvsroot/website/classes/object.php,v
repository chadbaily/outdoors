class object {

    var $GUID = null;
    
    /* {{{constructor
     *
     */
    function object() {
        $this->GUID = getRandomString(20);
    } //}}}

    /* {{{equals
     *
     */
    function equals($otherObject) { //boolean
        //return true if this object equals another one
    } //}}}

    /* {{{toString
     *
     */
    function toString() { //String
        return "[Default Implementation] object of type " . get_class($this);
    } //}}}

}
